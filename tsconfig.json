{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "typeRoots": ["./src/types"],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "baseUrl": "./src",
    "jsx": "react-jsx",
    "paths": {
      "~/*": ["*"]
    }
  },
  "include": [
    "src", ".eslintrc.js"
  ]
}
